<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal">

    <RelativeLayout
        android:id="@+id/fl_head1"
        android:layout_width="@dimen/qb_px_35"
        android:layout_height="@dimen/qb_px_35"
        android:background="@drawable/round_100_ffffff_all">

        <ImageView
            android:id="@+id/iv_head1"
            android:layout_width="@dimen/qb_px_30"
            android:layout_height="@dimen/qb_px_30"
            android:layout_centerInParent="true"
            tools:src="@drawable/ic_home_public" />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/fl_head2"
        android:layout_width="@dimen/qb_px_35"
        android:layout_height="@dimen/qb_px_35"
        android:background="@drawable/round_100_ffffff_all">

        <ImageView
            android:id="@+id/iv_head2"
            android:layout_width="@dimen/qb_px_30"
            android:layout_height="@dimen/qb_px_30"
            android:layout_centerInParent="true"
            android:src="@drawable/ic_home_public" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/fl_head3"
        android:layout_width="@dimen/qb_px_35"
        android:layout_height="@dimen/qb_px_35"
        android:background="@drawable/round_100_ffffff_all">

        <ImageView
            android:id="@+id/iv_head3"
            android:layout_width="@dimen/qb_px_30"
            android:layout_height="@dimen/qb_px_30"
            android:layout_centerInParent="true"
            android:src="@drawable/ic_home_public" />

    </RelativeLayout>

</LinearLayout>