<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/c_ff"
    android:orientation="vertical">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:src="@drawable/ic_launch_1" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:src="@drawable/ic_launch_2"
        app:layout_constraintBottom_toBottomOf="parent" />

    <ImageView
        android:layout_width="@dimen/qb_px_36"
        android:layout_height="@dimen/qb_px_630"
        android:layout_marginTop="@dimen/qb_px_230"
        android:src="@drawable/ic_launch_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="@dimen/qb_px_240"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/qb_px_170"
        android:adjustViewBounds="true"
        android:src="@drawable/ic_launch_4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</android.support.constraint.ConstraintLayout>
