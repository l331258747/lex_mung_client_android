<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEditText">
        <attr name="react_keys" format="string" />
        <attr name="sp_mode" format="enum">
            <enum name="integrated" value="0" />
            <enum name="breakable" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="SimpleFlowLayout">
        <attr name="hor_divider_width" format="dimension" />
        <attr name="hor_row_height" format="dimension" />
    </declare-styleable>


    <declare-styleable name="TitleView">
        <attr name="TitleView_title" format="string"/>
        <attr name="TitleView_right_text" format="string"/>
        <attr name="TitleView_right_img" format="reference"/>
        <attr name="TitleView_left_img" format="reference"/>
        <attr name="TitleView_bg" format="reference"/>
    </declare-styleable>

    <!--圆弧进度条-->
    <declare-styleable name="TasksCompletedView">
        <attr name="radius" format="dimension"/>
        <attr name="strokeWidth" format="dimension"/>
        <attr name="Completed_textColor" format="color"/>
        <attr name="circleColor" format="color"/>
        <attr name="ringColor" format="color"/>
        <attr name="ringBgColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="EmptyView">
        <attr name="EmptyView_bg" format="reference"/>
        <attr name="EmptyView_btn" format="string"/>
        <attr name="EmptyView_img" format="reference"/>
        <attr name="EmptyView_content" format="string"/>
        <attr name="EmptyView_content2" format="string"/>
        <attr name="EmptyView_content_text_color" format="reference"/>
    </declare-styleable>


    <declare-styleable name="FolderTextView">
        <attr name="foldline" format="integer" />
    </declare-styleable>

    <declare-styleable name="TabLayout">
        <attr name="tabIndicatorColor" format="color" />
        <attr name="tabIndicatorHeight" format="dimension" />
        <attr name="tabContentStart" format="dimension" />
        <attr name="tabBackground" format="reference" />
        <attr name="tabMode">
            <enum name="scrollable" value="0" />
            <enum name="fixed" value="1" />
        </attr>
        <attr name="tabGravity">
            <enum name="fill" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="tabMinWidth" format="dimension" />
        <attr name="tabMaxWidth" format="dimension" />
        <attr name="tabTextAppearance" format="reference" />
        <attr name="tabTextColor" format="color" />
        <attr name="tabSelectedTextColor" format="color" />
        <attr name="tabPaddingStart" format="dimension" />
        <attr name="tabPaddingTop" format="dimension" />
        <attr name="tabPaddingEnd" format="dimension" />
        <attr name="tabPaddingBottom" format="dimension" />
        <attr name="tabPadding" format="dimension" />
        <attr name="tabLineOffset" format="dimension" />
        <attr name="tabMarginBottom" format="dimension" />

    </declare-styleable>

</resources>